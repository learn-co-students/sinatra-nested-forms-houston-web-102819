<h1>Pirates<h1>
<div class="pirate">
<h3><%= @pirate.name %></h3><br>
<h4><%= @pirate.height %><h4><br>
<h4><%=@pirate.weight %><h4><br>
</div>

<h2>Ships</h2>
<% @ships.each do |ship| %>
<div class="ship">
 <p>Name: <%=ship.name%><p>
 <p>Type: <%=ship.type%><p>
 <p>Booty: <%=ship.booty%><p>
 </div>
 <%end%>

